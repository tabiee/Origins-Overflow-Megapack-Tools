{
	"name": "§3§n§lHP & Food Sync",
	"description": "Stop hitting yourself",
	"type": "origins:multiple",
	"hp": {
		"type": "origins:resource",
		"min": 0,
		"max": 20,
		"start_value": 20,
		"hud_render": {
			"should_render": true,
			"sprite_location": "origins:textures/gui/community/huang/resource_bar_01.png",
			"bar_index": 4,
			"condition": {
				"type": "origins:gamemode",
				"gamemode": "spectator"
			}
		}
	},
	"food": {
		"type": "origins:resource",
		"min": 0,
		"max": 20,
		"start_value": 20,
		"hud_render": {
			"should_render": true,
			"sprite_location": "origins:textures/gui/community/spiderkolo/resource_bar_02.png",
			"bar_index": 8,
			"condition": {
				"type": "origins:gamemode",
				"gamemode": "spectator"
			}
		}
	},
	"death": {
		"condition": {
			"type": "origins:health",
			"comparison": "==",
			"compare_to": 0
		},
		"type": "origins:action_over_time",
		"interval": 1,
		"rising_action": {
			"type": "origins:execute_command",
			"command": "kill @e[tag=duo]"
		}
	},
	"match_dmg": {
		"condition": {
			"type": "origins:resource",
			"resource": "sunmoon:sync_dmg",
			"comparison": "!=",
			"compare_to": 0
		},
		"type": "origins:action_over_time",
		"interval": 3,
		"entity_action": {
			"type": "origins:and",
			"actions": [
				{
					"type": "origins:execute_command",
					"command": "scale subtract pehkui:health 0.1 @s"
				},
				{
					"type": "origins:delay",
					"action": {
						"type": "origins:apply_effect",
						"effect": {
							"effect": "health_boost",
							"duration": 1,
							"amplifier": 0,
							"show_particles": false,
							"show_icon": false
						}
					},
					"ticks": 1
				}
			]
		}
	},
	"debug_dmg": {
		"condition": {
			"type": "origins:resource",
			"resource": "sunmoon:sync_dmg",
			"comparison": "==",
			"compare_to": 0
		},
		"type": "origins:action_over_time",
		"interval": 1,
		"entity_action": {
			"type": "origins:and",
			"actions": [
				{
					"type": "origins:execute_command",
					"command": "scale reset pehkui:health @s"
				}
			]
		}
	},
	"match_heal": {
		"condition": {
			"type": "origins:resource",
			"resource": "sunmoon:sync_heal",
			"comparison": "!=",
			"compare_to": 0
		},
		"type": "origins:action_over_time",
		"interval": 3,
		"entity_action": {
			"type": "origins:heal",
			"amount": 1
		}
	},
	"match_feed": {
		"condition": {
			"type": "origins:resource",
			"resource": "sunmoon:sync_feed",
			"comparison": "!=",
			"compare_to": 0
		},
		"type": "origins:action_over_time",
		"interval": 3,
		"entity_action": {
			"type": "origins:feed",
			"food": 1,
			"saturation": 0.5
		}
	},
	"match_exhaust": {
		"condition": {
			"type": "origins:resource",
			"resource": "sunmoon:sync_exhaust",
			"comparison": "!=",
			"compare_to": 0
		},
		"type": "origins:action_over_time",
		"interval": 3,
		"entity_action": {
			"type": "origins:exhaust",
			"amount": 4
		}
	},
	"check_values": {
		"type": "origins:action_over_time",
		"interval": 1,
		"entity_action": {
			"type": "origins:and",
			"actions": [
				{
					"type": "origins:execute_command",
					"command": "execute if score @e[tag=spec,limit=1] hp > @e[tag=cont,limit=1] hp run resource set @e[tag=spec,limit=1] sunmoon:sync_dmg 1"
				},
				{
					"type": "origins:execute_command",
					"command": "execute if score @e[tag=spec,limit=1] hp < @e[tag=cont,limit=1] hp run resource set @e[tag=spec,limit=1] sunmoon:sync_heal 1"
				},
				{
					"type": "origins:execute_command",
					"command": "execute if score @e[tag=spec,limit=1] food < @e[tag=cont,limit=1] food run resource set @e[tag=spec,limit=1] sunmoon:sync_feed 1"
				},
				{
					"type": "origins:execute_command",
					"command": "execute if score @e[tag=spec,limit=1] food > @e[tag=cont,limit=1] food run resource set @e[tag=spec,limit=1] sunmoon:sync_exhaust 1"
				}
			]
		}
	},
	"dmg": {
		"type": "origins:cooldown",
		"cooldown": 2,
		"hud_render": {
			"should_render": false,
			"bar_index": 2
		}
	},
	"heal": {
		"type": "origins:cooldown",
		"cooldown": 2,
		"hud_render": {
			"should_render": false,
			"bar_index": 4
		}
	},
	"feed": {
		"type": "origins:cooldown",
		"cooldown": 2,
		"hud_render": {
			"should_render": false
		}
	},
	"exhaust": {
		"type": "origins:cooldown",
		"cooldown": 2,
		"hud_render": {
			"should_render": false
		}
	}
}