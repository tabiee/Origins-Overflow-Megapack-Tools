{
    "name": "Rewind",
    "description": "Close ALL portals.",
    "type": "origins:multiple",
    "callback": {
        "type": "origins:action_on_callback",
        "execute_chosen_when_orb": false,
        "entity_action_removed": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "kill @e[tag=portal]",
                    "permission_level": 4
                },
                {
                    "type": "origins:execute_command",
                    "command": "kill @e[tag=point]",
                    "permission_level": 4
                },
                {
                    "type": "origins:execute_command",
                    "command": "kill @e[tag=trap]",
                    "permission_level": 4
                },
                {
                    "type": "origins:execute_command",
                    "command": "tag @e[tag=trapped] remove trapped",
                    "permission_level": 4
                },
                {
                    "type": "origins:delay",
                    "action": {
                        "type": "origins:execute_command",
                        "command": "forceload remove all",
                        "permission_level": 4
                    },
                    "ticks": 1
                }
            ]
        }
    },
    "act": {
        "type": "origins:active_self",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "kill @e[tag=portal]",
                    "permission_level": 4
                },
                {
                    "type": "origins:execute_command",
                    "command": "kill @e[tag=point]",
                    "permission_level": 4
                },
                {
                    "type": "origins:execute_command",
                    "command": "kill @e[tag=trap]",
                    "permission_level": 4
                },
                {
                    "type": "origins:execute_command",
                    "command": "tag @e[tag=trapped] remove trapped",
                    "permission_level": 4
                },
                {
                    "type": "origins:delay",
                    "action": {
                        "type": "origins:execute_command",
                        "command": "forceload remove all",
                        "permission_level": 4
                    },
                    "ticks": 1
                }
            ]
        },
        "cooldown": 10,
        "hud_render": {
            "should_render": false
        },
        "key": {
            "key": "key.saveToolbarActivator"
        }
    },
    "remove_too_far": {
        "condition": {
            "type": "origins:or",
            "conditions": [
                {
                    "type": "origins:command",
                    "command": "execute as @e[tag=ora] at @s if entity @e[tag=blu,distance=50..]",
                    "comparison": ">=",
                    "compare_to": 1
                },
                {
                    "type": "origins:command",
                    "command": "execute as @e[tag=blu] at @s if entity @e[tag=ora,distance=50..]",
                    "comparison": ">=",
                    "compare_to": 1
                }
            ]
        },
        "type": "origins:action_over_time",
        "interval": 1,
        "rising_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "title @s actionbar {\"text\":\"§3§lThe portals were too far apart, closing them both.§r\"}"
                },
                {
                    "type": "origins:execute_command",
                    "command": "execute as @e[tag=portal] at @s run particle end_rod ~ ~0.5 ~ 0.5 0.5 0.5 0.05 50",
                    "permission_level": 4
                },
                {
                    "type": "origins:execute_command",
                    "command": "execute as @e[tag=portal] at @s run summon item ~ ~ ~ {Age:5995,PickupDelay:200,Tags:[\"reset\"],Item:{id:\"minecraft:egg\",Count:1b}}",
                    "permission_level": 4
                },
                {
                    "type": "origins:delay",
                    "action": {
                        "type": "origins:execute_command",
                        "command": "kill @e[tag=portal]",
                        "permission_level": 4
                    },
                    "ticks": 1
                },
                {
                    "type": "origins:delay",
                    "action": {
                        "type": "origins:execute_command",
                        "command": "execute as @e[tag=reset] at @s run forceload remove ~ ~ ~ ~",
                        "permission_level": 4
                    },
                    "ticks": 2
                }
            ]
        }
    },
    "too_many_traps": {
        "condition": {
            "type": "origins:command",
            "command": "execute if entity @e[tag=trap]",
            "comparison": ">",
            "compare_to": 1
        },
        "type": "origins:action_over_time",
        "interval": 1,
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "kill @e[tag=trap,limit=1]"
                }
            ]
        }
    }
}